{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "vida-ahorro-app"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "assets/css/bootstrap/css/bootstrap.min.css",
        "assets/css/fontawesome-free/css/all.min.css",
        "assets/css/datatables/dataTables.bootstrap4.css",
        "assets/css/sbadmin/sb-admin.min.css"
      ],
      "scripts": [
        "assets/js/jquery/jquery.min.js",
        "assets/js/bootstrap/js/bootstrap.bundle.min.js",
        "assets/js/jquery-easing/jquery.easing.min.js",
        "assets/js/chart/Chart.min.js",
        "assets/js/datatables/jquery.dataTables.js",
        "assets/js/datatables/dataTables.bootstrap4.js",
        "assets/js/sb-admin/sb-admin.min.js",
        "assets/js/demo/datatables-demo.js",
        "assets/js/demo/chart-area-demo.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
